{
  "name": "bun-elysia-boilerplate",
  "version": "1.0.0",
  "description": "Bun + Elysia boilerplate with Drizzle, Postgres, and Redis",
  "main": "src/index.ts",
  "scripts": {
    "start": "bun run db:push && bun run src/index.ts",
    "prestart": "bun run db:push",
    "test": "bun test",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "debug": "bun run --inspect src/index.ts"
  },
  "dependencies": {
    "elysia": "latest",
    "drizzle-orm": "latest",
    "postgres": "latest",
    "bullmq": "latest",
    "ioredis": "latest",
    "bcryptjs": "^3.0.3",
    "jsonwebtoken": "^9.0.2",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "reflect-metadata": "^0.2.1",
    "winston": "^3.18.3",
    "zod": "^3.22.4",
    "drizzle-zod": "^0.5.1"
  },
  "devDependencies": {
    "bun-types": "latest",
    "drizzle-kit": "latest",
    "pg": "latest",
    "@types/pg": "latest",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "typescript": "^5.3.3"
  }
}